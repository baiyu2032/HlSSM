<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.dao.UserDao">
    <select id="selUserNum" parameterType="int" resultType="int">
        select usernum from user where usernum=#{num}
    </select>
    <select id="selUserNums" parameterType="int" resultType="String">
        select usernum from user where uopenid=#{uopenid}
    </select>
</mapper>