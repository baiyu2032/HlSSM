<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.dao.OrderDao">
    <insert id="addOrder" parameterType="cn.pojo.Order">
        INSERT INTO `order` VALUES(0,#{onum},#{ogoods},#{olocal},#{ologo},#{oremark},NULL,null)
    </insert>
    <select id="selOrder" resultType="cn.pojo.Order">
        SELECT onum,ogoods,olocal,ologo,oremark FROM `order`
    </select>
</mapper>